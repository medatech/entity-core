{
    "name": "@entity-core/pg",
    "version": "1.0.0",
    "description": "A postgress database driver for entity-core",
    "main": "./dist/index.js",
    "repository": "https://github.com/medatech/entity-core/tree/master/packages/pg",
    "author": "Martin Adams",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "test": "jest --runInBand src",
        "test:watch": "jest --runInBand --watch src",
        "coverage": "jest --runInBand --coverage src",
        "lint": "eslint ./src/**/*.ts",
        "lint:autofix": "eslint --fix ./src/**/*.ts"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@types/jest": "^26.0.10",
        "@types/node": "^14.6.0",
        "@types/pg": "^7.14.4",
        "@typescript-eslint/eslint-plugin": "^3.9.1",
        "@typescript-eslint/parser": "^3.9.1",
        "babel-jest": "^26.3.0",
        "eslint": "^7.7.0",
        "jest": "^26.4.0",
        "ts-jest": "^26.2.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "@entity-core/context": "^1.0.0",
        "@entity-core/datasource": "^1.0.0",
        "pg": "^8.3.0",
        "sql-template-strings": "^2.2.2"
    }
}
