{
    "name": "@entity-core/pg",
    "version": "1.0.0",
    "description": "A postgress database driver for entity-core",
    "main": "./dist/index.js",
    "repository": "https://github.com/medatech/entity-core/tree/master/packages/pg",
    "author": "Martin Adams",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "test": "TEST_ENV=unit jest src",
        "test:integration": "TEST_ENV=integration jest --runInBand src",
        "coverage": "jest --coverage src"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-typescript": "^7.8.3",
        "@entity-core/mock": "^1.0.0",
        "@types/jest": "^25.1.4",
        "@types/node": "^13.9.1",
        "@types/pg": "^7.14.2",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "babel-jest": "^25.1.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "ts-jest": "^25.2.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@entity-core/context": "^1.0.0",
        "@entity-core/datasource": "^1.0.0",
        "@entity-core/model": "^1.0.0",
        "pg": "^7.18.2",
        "sql-template-strings": "^2.2.2"
    }
}